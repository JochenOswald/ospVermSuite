<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryColor" Color="#6667AB"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#FBAD26"/>

    <SolidColorBrush x:Key="LightBackground" Color="#2D3135"/>
    <SolidColorBrush x:Key="BackgroundSelected" Color="#575B61"/>
    <SolidColorBrush x:Key="BackgroundNormal" Color="#2D3135"/>
    <SolidColorBrush x:Key="BackgroundHighlighted" Color="#42464A"/>
    <SolidColorBrush x:Key="Foreground" Color="#FFFFFF"/>
    
    <!-- Für Datagrid -->
    <SolidColorBrush x:Key="HeaderBackground" Color="#22252A"/>
    <SolidColorBrush x:Key="OptionOff" Color="#575A5D"/>
    <BitmapImage x:Key="DatagridChecked" UriSource="/Style/Dark/Datagrid_checked.png" />

    <!-- Buttons -->
    <SolidColorBrush x:Key="OutlineButtonText" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="OutlineButtonDown" Color="#000000"/>
    <SolidColorBrush x:Key="BackgroundMouseOver" Color="#42464A"/>
    <SolidColorBrush x:Key="BackgroundIsPressed" Color="#575B61"/>
    <SolidColorBrush x:Key="PrimaryColorHover" Color="#6E6FB8"/>
    <SolidColorBrush x:Key="PrimaryColorPressed" Color="#575891"/>

</ResourceDictionary>